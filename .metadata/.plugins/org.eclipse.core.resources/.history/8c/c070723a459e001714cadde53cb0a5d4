import java.io.*;
import java.util.Scanner;
/**
 * Class to read a given file 
 * @author James, Mohib
 *
 */
public class Read {

	private String a;
	private String b;
	private String c;
	private String d;
	private Scanner x;
	
	/**
	 * Constructor to initialize the fields
	 */
	public Read()
	{
		this.a = "";
		this.b = "";
		this.c = "";
		this.d = "";
		try{
			
			x = new Scanner(new File("TextFile.txt"));
		}
		catch (Exception e){
			System.out.println("could not find file"); 
		}
	}
	
	public String getA()
	{
		return a;
	}
	
	public String getB()
	{
		return b;
	}
	
	public String getC()
	{
		return c;
	}
	
	public String getD()
	{
		return d;
	}
}
