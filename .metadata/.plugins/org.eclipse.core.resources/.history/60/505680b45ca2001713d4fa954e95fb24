import java.util.ArrayList;
public class Calculate {

	private Read r = new Read();
	private ArrayList<Character> charArray;
	
	public Calculate()
	{
		this.charArray = new ArrayList<Character>();
	}
	
	public int findPlayerSpares()
	{
		int sum = 0;
		for(int i = 0; i < charArray.size(); i++) {
			if(charArray.get(i).toString().contains("/")) {
				sum += 1;
			}
		}
		return sum;
	}
	
